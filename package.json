{
  "name": "fapinstructor-client",
  "version": "2.0.1",
  "private": false,
  "homepage": "https://fapinstructor.com/",
  "license": "MIT",
  "prepare": "npx husky install",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true yarn test --bail --findRelatedTests",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.snap --max-warnings=0 .",
    "prettier": "prettier --ignore-path .gitignore  \"**/*.+(js|ts|tsx|json|yml|md)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc --noEmit",
    "validate": "CI=true npm-run-all --parallel check-types test check-format lint",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.6.0",
    "@sentry/react": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.4.0",
    "@xstate/test": "^0.4.2",
    "ajv": "^8.6.0",
    "axios": "^0.21.1",
    "clipboard": "^2.0.8",
    "clsx": "^1.1.1",
    "date-fns": "^2.22.1",
    "fast-memoize": "^2.5.2",
    "formik": "^2.2.9",
    "formik-material-ui": "^3.0.1",
    "github-markdown-css": "^4.0.0",
    "lodash.shuffle": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.29.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.2",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "standardized-audio-context": "^25.2.9",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.4",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2",
    "xstate": "^4.20.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.1",
    "@storybook/addon-essentials": "^6.3.1",
    "@storybook/addon-links": "^6.3.1",
    "@storybook/addon-storyshots": "^6.3.1",
    "@storybook/node-logger": "^6.3.1",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-motion": "^0.0.29",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.10",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.16.0",
    "@types/yup": "^0.29.11",
    "babel-plugin-polyfill-corejs2": "^0.2.2",
    "babel-plugin-polyfill-regenerator": "^0.2.2",
    "chromatic": "^5.9.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-testing-library": "^4.6.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^11.0.0",
    "mockdate": "^3.0.5",
    "msw": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2"
  }
}
