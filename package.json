{
  "name": "fapinstructor-client",
  "version": "2.0.1",
  "private": false,
  "homepage": "https://fapinstructor.com/",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true yarn test --bail --findRelatedTests",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.snap . --max-warnings=0",
    "prettier": "prettier --ignore-path .gitignore  \"**/*.+(js|ts|tsx|json|yml|md)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc --noEmit",
    "validate": "CI=true npm-run-all --parallel check-types test check-format lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "@reduxjs/toolkit": "^1.3.6",
    "@sentry/react": "^5.29.0",
    "@xstate/inspect": "^0.4.1",
    "@xstate/react": "^1.3.3",
    "ajv": "^6.5.2",
    "axios": "^0.19.2",
    "clipboard": "^2.0.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "fast-memoize": "^2.5.1",
    "fetch-jsonp": "^1.1.3",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "github-markdown-css": "^4.0.0",
    "lodash.shuffle": "^4.2.0",
    "moment": "^2.20.1",
    "query-string": "^6.13.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-markdown": "^4.3.1",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.0.1",
    "standardized-audio-context": "^24.1.20",
    "styled-components": "^5.2.0",
    "typescript": "^3.9.2",
    "uuid": "^8.1.0",
    "whatwg-fetch": "^3.6.2",
    "xstate": "^4.19.1",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.28",
    "@storybook/node-logger": "^6.0.26",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-motion": "^0.0.29",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^8.0.0",
    "@types/webpack-env": "^1.15.2",
    "@types/yup": "^0.29.0",
    "babel-loader": "^8.1.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-testing-library": "^3.10.0",
    "faker": "^5.1.0",
    "husky": "^4.2.3",
    "jest-fetch-mock": "^3.0.1",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.0.7",
    "mockdate": "^3.0.2",
    "msw": "^0.28.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "react-test-renderer": "^17.0.1"
  }
